{
  "name": "continuousphp/sdk",
  "homepage": "https://github.com/continuousphp/sdk",
  "description": "PHP SDK to consume continuousphp API",
  "type": "library",
  "keywords": [
    "build",
    "tool",
    "continuousphp"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Frederic Dewinne",
      "email": "frederic@continuousphp.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^4.6",
    "squizlabs/php_codesniffer": "^2.3",
    "behat/behat": "^3.0",
    "php-mock/php-mock-phpunit": "^1.1"
  },
  "require": {
    "php": ">=5.4.0",
    "guzzlehttp/guzzle-services": "~1.1.0"
  },
  "autoload": {
    "psr-4": {
      "Continuous\\Sdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Continuous\\Tests\\Sdk\\": "tests/Tests",
      "Continuous\\Features\\": "features/bootstrap/",
      "Continuous\\Fixtures\\Sdk\\": "tests/Fixtures"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Pierozi/guzzle-services.git"
    }
  ]
}
